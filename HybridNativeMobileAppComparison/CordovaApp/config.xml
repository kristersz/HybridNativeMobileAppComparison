<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.cordova.CordovaShowcase" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>Cordova Showcase</name>
  <description>A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">Kristers Zimecs </author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.1" />
  <preference name="windows-phone-target-version" value="8.1" />
  <vs:plugin name="org.apache.cordova.camera" version="0.3.2" />
  <vs:plugin name="org.apache.cordova.console" version="0.2.11" />
  <vs:plugin name="org.apache.cordova.device-motion" version="0.2.10" />
  <vs:plugin name="org.apache.cordova.battery-status" version="0.2.11" />
  <vs:plugin name="org.apache.cordova.contacts" version="0.2.13" />
  <vs:plugin name="com.ionic.keyboard" version="1.0.4" />
  <vs:plugin name="org.apache.cordova.device" version="0.2.12" />
  <vs:plugin name="org.apache.cordova.device-orientation" version="0.3.9" />
  <vs:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
  <vs:plugin name="org.apache.cordova.network-information" version="0.2.12" />
  <vs:plugin name="org.apache.cordova.vibration" version="0.3.11" />
  <vs:plugin name="org.apache.cordova.dialogs" version="0.2.10" />
  <vs:plugin name="org.apache.cordova.globalization" version="0.3.1" />
  <vs:plugin name="org.apache.cordova.statusbar" version="0.1.8" />
  <vs:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
  <vs:platformSpecificValues />
</widget>